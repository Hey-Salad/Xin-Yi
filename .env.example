# =============================================================================
# HeySalad Platform Configuration Template
# Copy this file to .env and fill in your actual values
# =============================================================================

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-role-key-here

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True
BACKEND_PORT=2124
FRONTEND_PORT=2125

# =============================================================================
# AI Model APIs
# =============================================================================

# DeepSeek
DEEPSEEK_API_KEY=your-deepseek-key
DEEPSEEK_MODEL=deepseek-chat

# Anthropic (Claude)
ANTHROPIC_API_KEY=your-anthropic-key
ANTHROPIC_MODEL=claude-3-opus-20240229

# Google Gemini
GEMINI_API_KEY=your-gemini-key
GEMINI_MODEL=gemini-1.5-flash

# OpenAI
OPENAI_API_KEY=your-openai-key
OPENAI_MODEL=gpt-4o-mini

# HuggingFace
HUGGINGFACE_API_KEY=your-huggingface-key
HUGGINGFACE_MODEL=openai/gpt-oss-120b

# Cloudflare AI
CLOUDFLARE_API_KEY=your-cloudflare-key

# AWS Bedrock
AWS_BEARER_TOKEN_BEDROCK=your-bedrock-token

# =============================================================================
# Payment Processing
# =============================================================================

# Stripe
STRIPE_PUBLIC_KEY=pk_test_your-public-key
STRIPE_SECRET_KEY=sk_test_your-secret-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# =============================================================================
# Communication Services
# =============================================================================

# SendGrid (Email)
SENDGRID_API_KEY=your-sendgrid-key
SENDGRID_FROM_EMAIL=noreply@yourdomain.com

# Twilio (SMS/Voice)
TWILIO_ACCOUNT_SID=your-account-sid
TWILIO_AUTH_TOKEN=your-auth-token
TWILIO_API_KEY_SID=your-api-key-sid
TWILIO_API_KEY_SECRET=your-api-key-secret
TWILIO_AUTH_TOKEN_SID=your-auth-token-sid
TWILIO_PHONE_NUMBER=+1234567890
TWILIO_SIP_DOMAIN=yourdomain.sip.twilio.com
